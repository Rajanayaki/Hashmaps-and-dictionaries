
def is_cube(n):
    cube_root = n**(1/3)
    if round(cube_root) ** 3 == n:
        return True
    else:
        return False
def is_square(n):
  root = n**(1/2)
  if round(root)**2==n:
    return True
  else:
    return False
number = int(input("Enter the Number:  "))
for i in range(number,0,-1):
  if is_square(i) and is_cube(i):
    print(i)
    break
